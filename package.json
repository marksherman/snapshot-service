{
  "name": "snapshot-service",
  "version": "1.1.0",
  "description": "Server to receive and store code snapshots for learning data analysis. Designed for [MIT App Inventor](http://appinventor.mit.edu/explore/), but can be adapted to work for any text-based data collection.",
  "main": "server/snapshot-service.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marksherman/snapshot-service.git"
  },
  "author": "Mark Sherman <shermanm@mit.edu>",
  "license": "GPL-3.0",
  "contributors": [
    "Kim Douglas (https://github.com/kmbrlynn)",
    "Derrell Lipman (https://github.com/derrell)"
  ],
  "bugs": {
    "url": "https://github.com/marksherman/snapshot-service/issues"
  },
  "scripts": {
    "start": "forever start -m 1 --minUptime 1000 --spinSleepTime 1000 snapshot-service.js",
    "stop": "forever stopall",
    "restart": "forever restartall",
    "test": "mocha --reporter spec"
  },
  "homepage": "https://github.com/marksherman/snapshot-service#readme",
  "dependencies": {
    "codename": "0.0.4",
    "cors": "^2.7.1",
    "express": "^4.16.3",
    "forever": "^0.15.3",
    "jrpc2": "^1.0.5",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "multer": "^1.3.0",
    "path": "^0.12.7",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.1.1",
    "rewire": "^3.0.2"
  },
  "engines": {
    "node": ">=8.9.4 <9.0"
  },
  "keywords": [
    "server",
    "research",
    "snapshot",
    "instrumentation",
    "deidentification"
  ]
}
